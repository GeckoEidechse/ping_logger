{
   "Name": "Gecko.ping_logger",
   "Description": "A Northstar server mod that logs the ping of all players in a periodic interval",
   "RequiredOnClient": false,
   "Version": "0.0.1",
   "DownloadLink": "https://northstar.thunderstore.io/package/download/Example/Mod/",
   "LoadPriority": 0,
   "ConVars": [
      {
         "Name": "log_ping_interval",
         "DefaultValue": "1"
      }
   ],
   "Scripts": [
      {
         "Path": "ping_log.nut",
         "RunOn": "SERVER",
         "ServerCallback": {
            "After": "ping_log"
         }
      }
   ],
   "Localisation": []
}
